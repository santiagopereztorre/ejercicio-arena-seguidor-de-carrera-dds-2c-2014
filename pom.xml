<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

   <modelVersion>4.0.0</modelVersion>
   <groupId>dds-templates</groupId>
   <artifactId>seguidor-de-carrera</artifactId>

   <parent>
      <groupId>org.uqbar-project</groupId>
      <artifactId>uqbar-parent-project</artifactId>
      <version>1.7</version>
   </parent>

  <dependencies>
    <dependency>
      <groupId>org.uqbar-project</groupId>
      <artifactId>arena-jface</artifactId>
      <version>3.3-SNAPSHOT</version>
      <exclusions>
        <exclusion>
          <artifactId>javassist</artifactId>
          <groupId>javassist</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>org.javassist</groupId>
      <artifactId>javassist</artifactId>
      <version>3.18.1-GA</version>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  
  
  
    <!-- Los repositorios son sÃ³lo necesarios si no fueron agregados ya al 
    settings.xml como se explica en http://uqbar-wiki.org/index.php?title=Configuraci%C3%B3n_de_Maven_para_poder_utilizar_las_herramientas_de_Uqbar -->
  <repositories>
    <repository>
      <id>uqbar-wiki.org-releases</id>
      <name>uqbar-wiki.org-releases</name>
      <url>http://uqbar-wiki.org/mvn/releases</url>
    </repository>
    <repository>
      <snapshots />
      <id>uqbar-wiki.org-snapshots</id>
      <name>uqbar-wiki.org-snapshots</name>
      <url>http://uqbar-wiki.org/mvn/snapshots</url>
    </repository>
  </repositories>

</project>
